{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Score TTS",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scoring/entrypoint.py",
            "args": [
                "tts",
                "parler-tts-mini-expresso",
                "parler-tts",
                "default",
                "17226295660308663618"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
